<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HeaderCardView">
        <attr name="header_card_title" format="string"/>
        <attr name="header_card_subtitle" format="string"/>
        <attr name="header_card_overline" format="string"/>
    </declare-styleable>

    <declare-styleable name="PreviewCardView">
        <attr name="preview_card_leading_drawable" format="reference"/>
        <attr name="preview_card_title" format="string"/>
        <attr name="preview_card_subtitle" format="string"/>
    </declare-styleable>

    <declare-styleable name="UniversityCardView">
        <attr name="university_name" format="string"/>
    </declare-styleable>

    <declare-styleable name="TeacherCardView">
        <attr name="teacher_name" format="string"/>
    </declare-styleable>

    <declare-styleable name="FacultyCardView">
        <attr name="faculty_name" format="string"/>
        <attr name="faculty_professors_count" format="integer"/>
    </declare-styleable>

    <declare-styleable name="ProfessorCardView">
        <attr name="professor_name" format="string"/>
        <attr name="professor_reviews_count" format="integer"/>
    </declare-styleable>

    <declare-styleable name="SubjectReviewsCardView">
        <attr name="subject_reviews_name" format="string"/>
        <attr name="subject_reviews_count" format="integer"/>
    </declare-styleable>
</resources>